{
  "backend": "callback",
  "root_dir": "/workspace",
  "files": {
    "/workspace/input.txt": "hello",
    "/workspace/out.txt": ""
  },
  "permissions": {
    "/workspace/input.txt": "read",
    "/workspace/out.txt": "write"
  },
  "default_permission": "deny",
  "code": "filesystem seeded sequence",
  "inputs": {
    "operations": [
      {
        "op": "read_text",
        "path": "/workspace/input.txt"
      },
      {
        "op": "write_text",
        "path": "/workspace/out.txt",
        "data": "HELLO"
      }
    ],
    "inspect_paths": [
      "/workspace/input.txt",
      "/workspace/out.txt"
    ]
  }
}
